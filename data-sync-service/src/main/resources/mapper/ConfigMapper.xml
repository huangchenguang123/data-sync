<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chuxing.datasyncservice.dao.ConfigDAO">

    <resultMap id="BaseResultMap" type="com.chuxing.datasyncservice.model.dto.ConfigDTO">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="componentId" column="component_id" jdbcType="BIGINT"/>
        <result property="config" column="config" jdbcType="VARCHAR"/>
        <result property="configType" column="config_type" jdbcType="VARCHAR"/>
        <result property="subType" column="sub_type" jdbcType="VARCHAR"/>
        <result property="createAt" column="create_at" jdbcType="BIGINT"/>
        <result property="updateAt" column="update_at" jdbcType="BIGINT"/>
    </resultMap>

    <select id="getByComponentId" resultMap="BaseResultMap">
        select *
        from config
        where component_id = #{componentId}
        ;
    </select>

</mapper>
