<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chuxing.datasyncservice.dao.ComponentDAO">

    <resultMap id="BaseResultMap" type="com.chuxing.datasyncservice.model.dto.ComponentDTO">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="createAt" column="create_at" jdbcType="BIGINT"/>
        <result property="updateAt" column="update_at" jdbcType="BIGINT"/>
    </resultMap>

    <select id="getAllComponent" resultMap="BaseResultMap">
        select *
        from component
        ;
    </select>

</mapper>
